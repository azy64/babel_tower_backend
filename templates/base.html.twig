<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="{{ asset('images/babel2.png')}}">
        <link href="{{ asset('css/bootstrap.css')}}" rel="stylesheet" crossorigin="anonymous">
        <link href="{{ asset('css/style.css')}}" rel="stylesheet" crossorigin="anonymous">
        <script type='text/javascript' src="{{asset('js/lecteur.js')}}"></script>

        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
